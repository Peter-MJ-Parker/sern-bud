datasource db {
    provider = "mongodb"
    url      = env("CONNECT")
}

generator client {
    provider = "prisma-client-js"
}

model Money {
    id       String  @id @map("_id")
    username String
    nickname String?
    userID   String
    bank     Int     @default(0)
    wallet   Int     @default(0)
}

{
  "name": "budbot",
  "version": "4.0.0",
  "description": "Discord bot for the Joint Streaming Community.",
  "type": "module",
  "main": "dist/Structures/client/index.js",
  "scripts": {
    "build": "tsc",
    "tsc:watch": "tsc -w",
    "start/dev": "tsc && nodemon ./dist/Structures/client/index.js",
    "start/node": "tsc && node ./dist/Structures/client/index.js"
  },
  "keywords": [
    "typescript",
    "sern",
    "discord.js",
    "economy"
  ],
  "license": "UNLICENSED",
  "dependencies": {
    "@napi-rs/canvas": "^0.1.53",
    "@sern/handler": "^4.0.1",
    "@spark.ts/logger": "^1.4.0",
    "axios": "^1.7.2",
    "chalk": "^5.3.0",
    "colorette": "^2.0.20",
    "discord.js": "^14.15.3",
    "mongoose": "^8.5.1"
  },
  "devDependencies": {
    "@types/node": "^20.14.12",
    "nodemon": "^3.1.4",
    "typescript": "^5.5.4"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Peter-MJ-Parker/sern-bud.git"
  },
  "imports": {
    "#plugins": [
      "./dist/Structures/plugins/index.js"
    ],
    "#BudBot": [
      "./dist/Structures/client/BudBot.js"
    ],
    "#schemas/*": [
      "./dist/Structures/schemas/*.js"
    ]
  }
}
